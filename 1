#include"header.h"
#include"DS.h"
#include"declaration.h"

void* (*afp[NOF])(void*);
void* mainMenu(void*);
void* exitProgram(void*);

void* creatLinklist(void*);

void* insertNode(void*);
void* deleteNode(void*);
void* displayLinklist(void*);
void* sortLinklist(void*);
void* rotateLinklist(void*);

void* insertBeg(void*);
void* insertEnd(void*);
void* insertNth(void*);
void* insertkey(void*);

void* creatNode(void*);


void* deleteBeg(void*);
void* deleteEnd(void*);
void* deleteNth(void*);
void* deletekey(void*);

int init()
{
#ifdef DEBUG
        printf("%s: Begin.\n",__func__);
#endif
	afp[0] = mainMenu;
	afp[1] = exitProgram;

	afp[2] = creatLinklist;

	afp[3] = insertNode;
	afp[4] = deleteNode;
	afp[5] = displayLinklist;
	afp[6] = sortLinklist;
	afp[7] = rotateLinklist;

	afp[8]  =  insertBeg;
	afp[9]	=  insertEnd;
	afp[10] =  insertNth;
	afp[11] =  insertkey;

	afp[12] = creatNode;

	afp[13]  =  deleteBeg;
	afp[14]	 =  deleteEnd;
	afp[15]  =  deleteNth;
	afp[16]  =  deletekey;

#ifdef DEBUG
        printf("%s: End.\n",__func__);
#endif
	return 0;
}
